{
  "properties": [
    {
      "name": "spring.zookeeper.host",
      "type": "java.lang.String",
      "description": "Zookeeper host."
    },
    {
      "name": "spring.zookeeper.retryPolicy",
      "type": "java.lang.String",
      "description": "exponential-backoff-retry:重试指定的次数, 且每一次重试之间停顿的时间逐渐增加 retry-n-times:指定最大重试次数的重试策略 retry-one-time:只重试一次 retry-forever:永远重试 retry-until-elapsed:一直重试直到达到规定的时间",
      "defaultValue": "retry-until-elapsed"
    },
    {
      "name": "spring.zookeeper.sessionTimeOut",
      "type": "java.lang.Integer",
      "description": "Zookeeper sessionTimeOut.",
      "defaultValue": 5000
    },
    {
      "name": "spring.zookeeper.connectTimeOut",
      "type": "java.lang.Integer",
      "description": "Zookeeper connectTimeOut.",
      "defaultValue": 5000
    },
    {
      "name": "spring.zookeeper.nameSpace",
      "type": "java.lang.String",
      "description": "Zookeeper nameSpace."
    }
  ]
}